vec2 uv_fract = fract(uv_in);

cellsMinimum = mod(mod(cellsMinimum, modPeriod) + modPeriod, modPeriod);
cellsMaximum = mod(mod(cellsMaximum, modPeriod) + modPeriod, modPeriod);

vec2 blur = smoothstep(0.0, 1.0, uv_fract);

vec2 lowerLeftDirection = vec2(cellsMinimum.x, cellsMinimum.y);
vec2 lowerRightDirection = vec2(cellsMaximum.x, cellsMinimum.y);
vec2 upperLeftDirection = vec2(cellsMinimum.x, cellsMaximum.y);
vec2 upperRightDirection = vec2(cellsMaximum.x, cellsMaximum.y);

